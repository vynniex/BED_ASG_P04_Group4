            "required": true,
            "schema": {
              "type": "string",
              "properties": {
                "clinic": {
                  "example": "Paris Ris Polyclinic"
                }
              }
            }
          },
          {
            "name": "reason",
            "in": "body",
            "description": "Reason for the appointment",
            "required": true,
            "schema": {
              "type": "string",
              "properties": {
                "reason": {
                  "example": "General Consultation"
                }
              }
            }
          }
        ],
